{
    "name": "xynha/php-assert",
    "description": "Assertion done right. Static analyzer friendly assertion library.",
    "keywords": [
        "assert",
        "check",
        "validate"
    ],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Asis Pattisahusiwa",
            "email": "asis.pattisahusiwa@gmail.com"
        }
    ],
    "homepage": "https://github.com/pattisahusiwa/php-assert",
    "support": {
        "issues": "https://github.com/pattisahusiwa/php-assert/issues"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Xynha\\Assert\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xynha\\Assert\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.1 || ^8.0"
    },
    "require-dev": {},
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
